{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch SCHEDULER",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/scheduler.ts",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch PROGRAM",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/main.ts",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "console": "integratedTerminal"
    }
  ]
}
